<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="studio_customization.odoo_studio_book_lis_ffe25e61-1094-4821-82bd-0c74ad3b09bb" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//tree[1]/field[@name='name']" position="before">
          <field name="isbn"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="library_app.view_tree_book"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">library.book</field>
    <field name="name">Odoo Studio: Book List customization</field>
    <field name="priority">99</field>
    <field name="type">tree</field>
  </record>
  <record id="studio_customization.odoo_studio_book_for_8f81c870-bfaf-4ce1-ad96-875fb33af618" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//form[1]/sheet[1]/group[1]/field[@name='name']" position="after">
          <xpath expr="//field[@name='isbn']" position="move"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="library_app.view_form_book"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">library.book</field>
    <field name="name">Odoo Studio: Book Form customization</field>
    <field name="priority">99</field>
    <field name="type">form</field>
  </record>
</odoo>
